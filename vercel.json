{
  "version": 2,
  "builds": [
    {
      "src": "main.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.go"
    }
  ],
  "env": {
    "ENV": "production",
    "PORT": "8080"
  },
  "functions": {
    "main.go": {
      "maxDuration": 30
    }
  }
} 